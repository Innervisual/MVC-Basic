@{
    ViewBag.Title = "Home Page";
}

@{
    ViewData["Title"] = "Temperature";
}


<h1>Fever or not</h1>
<form method="post" asp-controller="Doctor" asp-action="Temperature">
    <label>Enter your temperature (in degree Celcius) to check if you have fever or not:</label><br>
    <input type="text" name="Temp"><br>
    <input type="submit" value="Submit">
</form>

@{
    ViewBag.Title = "Fevercheck";
}
@if (ViewBag.LinkableId != null)
{
    @foreach (var item in ViewBag.LinkableId)
    {
        <h1>@item</h1>
    }
}